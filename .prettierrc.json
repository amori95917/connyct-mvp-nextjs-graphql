	"trailingComma": "es5",
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"printWidth": 100,
	"bracketSpacing": true,
	"tabWidth": 1,
	"arrowParens": "avoid",
	"jsxBracketSameLine": true,
	"sortClassNamesUnknownClassesSeparator": "",
